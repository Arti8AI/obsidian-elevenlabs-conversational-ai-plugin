{
	"name": "obsidian-elevenlabs-conversational-ai-plugin",
	"version": "0.0.4",
	"description": "ElevenLabs Conversational AI Plugin for Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"lint": "eslint src --ext .ts,.tsx"
	},
	"keywords": [
		"AI",
		"Agent",
		"Conversational",
		"Text-to-Speech",
		"Speech-to-Text",
		"Voice",
		"ElevenLabs"
	],
	"author": "Arti8",
	"license": "MIT",
	"dependencies": {
		"@11labs/client": "latest",
		"eslint": "^8.57.1"
	},
	"devDependencies": {
		"@types/jest": "^29.5.8",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jest": "^29.7.0",
		"obsidian": "latest",
		"ts-jest": "^29.1.1",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"roots": ["<rootDir>/src", "<rootDir>/tests"],
		"testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
		"collectCoverageFrom": [
			"src/**/*.ts",
			"!src/**/*.d.ts"
		]
	}
}
